    "settings": {
      "foreground": "#d35d41"
    }
  },
  {
    "scope": "storage.type.java",
    "settings": {
      "foreground": "#fabd2f"
    }
  },
  {
    "scope": "storage.type.annotation",
    "settings": {
      "foreground": "#d08f48",
      "fontStyle": "bold"
    }
  },
  {
    "scope": "keyword.other.documentation.javadoc",
    "settings": {
      "foreground": "#d35d41"
    }
  },
  {
    "scope": "comment.block.javadoc variable.parameter.java",
