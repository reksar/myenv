{

"name": "Motley",
"type": "dark",

"tokenColors": [

  {
    "settings": {
      "foreground": "#baae8f",
      "background": "#21201d"
    }
  },

  {
    "scope": [
      "emphasis",
      "variable.parameter",
      "meta.parameter.type.variable",
      "meta.function-call.arguments"
    ],
    "settings": {
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "strong",
      "keyword",
      "keyword.control",
      "keyword.operator",
      "storage.modifier",
      "punctuation.definition.tag.xml"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "constant",
      "support.constant",
      "constant.rgb-value",
      "string.tag",
      "string.value",
      "string.escape",
      "meta.selector",
      "entity.name.selector",
      "variable.arguments"
    ],
    "settings": {
      "foreground": "#d35d41"
    }
  },

  {
    "scope": [
      "variable",
      "variable.name",
      "variable.other",
      "string.constant.other.placeholder",
      "entity.name.variable.field",
      "entity.name.variable.property",
      "entity.name.variable.local",
      "entity.name.variable.parameter",
      "entity.other.attribute-name.localname.xml"
    ],
    "settings": {
      "foreground": "#d08f48"
    }
  },

  {
    "scope": [
      "string",
      "punctuation.definition.string.begin",
      "punctuation.definition.string.end",
      "entity.name.scope-resolution",
      "entity.name.type.namespace"
    ],
    "settings": {
      "foreground": "#ebdbb2"
    }
  },

  {
    "scope": [
      "entity.name.method",
      "entity.name.method.function-call",
      "entity.name.function.call",
      "entity.name.function.definition",
      "entity.name.function.cs"
    ],
    "settings": {
      "foreground": "#e0aa2b"
    }
  },

  {
    "scope": [
      "keyword.other.unit",
      "meta.preprocessor.string",
      "meta.preprocessor.numeric",
      "entity.name.tag.localname.xml",
      // Python (py)
      "source.python meta.function.python",
      "source.python entity.name.function.python",
      "source.python meta.function-call.generic",
      // Markdown (md)
      "markup.inserted",
      // Git
      "constant.sha.git-rebase"
    ],
    "settings": {
      "foreground": "#9e9e39"
    }
  },

  {
    "scope": [
      "support.function.builtin",
      // Python (py)
      "source.python entity.name.function.decorator",
      "source.python support.type.python"
    ],
    "settings": {
      "foreground": "#9e9e39",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "comment",
      "punctuation.definition.comment"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "punctuation.quoted",
      // Python (py)
      "source.python string.quoted.docstring.multi.python",
      "source.python string.quoted.double.block.python",
      "source.python punctuation.definition.string.begin.python",
      "source.python punctuation.definition.string.end.python"
    ],
    "settings": {
      "foreground": "#777777"
    }
  },

  {
    "scope": [
      "*url*",
      "*link*",
      "*uri*"
    ],
    "settings": {
      "fontStyle": "underline"
    }
  },

  {
    "scope": "header",
    "settings": {
      "foreground": "#458588"
    }
  },

  {
    "scope": [
      "invalid",
      "invalid.illegal"
    ],
    "settings": {
      "foreground": "#cc241d"
    }
  },

  {
    "scope": "invalid.deprecated",
    "settings": {
      "foreground": "#b16286"
    }
  },

  {
    "scope": "meta.header.diff",
    "settings": {
      "foreground": "#fe8019"
    }
  },

  {
    "scope": "string.regexp",
    "settings": {
      "foreground": "#fe8019"
    }
  },

  {
    "scope": "module.node",
    "settings": {
      "foreground": "#d08f48"
    }
  },

  {
    "scope": "support.type.property-name",
    "settings": {
      "foreground": "#689d6a"
    }
  },

  {
    "scope": "keyword.control.less",
    "settings": {
      "foreground": "#d79921"
    }
  },

  {
    "scope": "support.function.git-rebase",
    "settings": {
      "foreground": "#689d6a"
    }
  },

  {
    "scope": [
      "meta.return-type",
      "meta.return.type",
      "meta.type.name",
      "meta.type.annotation",
      "meta.cast"
    ],
    "settings": {
      "foreground": "#af875f",
      "fontStyle": "bold"
    }
  },

  {
    "name": "this",
    "scope": [
      "variable.this",
      "support.variable"
    ],
    "settings": {
      "foreground": "#d3869b"
    }
  },


  {
    "name": "default type",
    "scope": [
      "keyword.type",
      "support.type",
      "storage.type.built-in.primitive"
    ],
    "settings": {
      "foreground": "#afaf3f",
      "fontStyle": "none"
    }
  },

  {
    "name": "class name",
    "scope": [
      "entity.name.class",
      "entity.name.type.class",
      "storage.type.cs"
    ],
    "settings": {
      "foreground": "#afaf3f",
      "fontStyle": "none"
    }
  },

  {
    "scope": "prototype",
    "settings": {
      "foreground": "#d3869b"
    }
  },

  // ------------------------------------------------------------------------
  // LANGUAGE SPECIFIC
  // ------------------------------------------------------------------------

  // --- Python (py) --------------------------------------------------------

  {
    "name": "function and class definition keywords",
    "scope": [
      "source.python storage.type.function.python",
      "source.python storage.modifier.declaration",
      "source.python storage.type.class.python"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "bold"
    }
  },

  {
    "name": "function arguments",
    "scope": [
      "source.python meta.function-call.arguments"
    ],
    "settings": {
      "foreground": "#d08f48"
    }
  },

  {
    "name": "all CAPS",
    "scope": "source.python constant.other.caps",
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "normal"
    }
  },

  {
    "name": "Python string",
    "scope": [
      "source.python string.quoted.single.single-line.python"
    ],
    "settings": {
      "foregroung": "#d35d41"
    }
  },

  {
    "name": "self, cls",
    "scope": [
      "source.python variable.language.special.self.python",
      "source.python variable.parameter.function.language.special.self.python",
      "source.python variable.language.special.cls.python",
      "source.python variable.parameter.function.language.special.cls.python"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  },

  {
  "name": "logical operator",
    "scope": [
      "source.python keyword.operator.logical.python",
      "source.python constant.language.python"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "bold"
    }
  },

  // --- Markdown (md) -------------------------------------------------------

  {
    "scope": "markup.underline",
    "settings": {
      "fontStyle": "underline"
    }
  },
  {
    "scope": "string.other.link.title.markdown",
    "settings": {
      "foreground": "#928374",
      "fontStyle": "underline"
    }
  },
  {
    "scope": "markup.underline.link",
    "settings": {
      "foreground": "#d3869b"
    }
  },
  {
    "scope": "markup.bold",
    "settings": {
      "fontStyle": "bold"
    }
  },
  {
    "scope": "markup.heading",
    "settings": {
      "fontStyle": "bold",
      "foreground": "#fe8019"
    }
  },
  {
    "scope": "markup.italic",
    "settings": {
      "fontStyle": "italic"
    }
  },
  {
    "scope": "markup.deleted",
    "settings": {
      "foreground": "#d65d0e"
    }
  },
  {
    "scope": "markup.changed",
    "settings": {
      "foreground": "#fe8019"
    }
  },
  {
    "scope": "markup.punctuation.quote.beginning",
    "settings": {
      "foreground": "#98971a"
    }
  },
  {
    "scope": "markup.punctuation.list.beginning",
    "settings": {
      "foreground": "#d08f48"
    }
  },
  {
    "scope": [
      "markup.inline.raw",
      "markup.fenced_code.block"
    ],
    "settings": {
      "foreground": "#d35d41"
    }
  },

  // C# (cs) -----------------------------------------------------------------

  {
    "scope": [
      "keyword.other.get.cs",
      "keyword.other.set.cs"
    ],
    "settings": {
      "foreground": "#689d6a",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "variable.other.object.property.cs"
    ],
    "settings": {
      "foreground": "#baae8f"
    }
  },

  // C / C++ (c cpp) ---------------------------------------------------------

  {
    "name": "typedef constant",
    "scope": "meta.body.function.definition.cpp",
    "settings": {
      "foreground": "#689d6a"
    }
  },

  {
    "scope": "support.function.C99",
    "settings": {
      "foreground": "#fabd2f"
    }
  },

  {
    "scope": [
      "storage.type.class.cpp",
      "storage.type.struct.cpp"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  },

  // Assembler (asm) ---------------------------------------------------------

  {
    "scope": "source.asm.x86_64 entity.name.section",
    "settings": {
      "fontStyle": "bold"
    }
  },

  {
    "scope": "entity.name.function.asm",
    "settings": {
      "foreground": "#afaf3f"
    }
  },

  {
    "scope": "support.function.asm",
    "settings": {
      "foreground": "#e0aa2b"
    }
  },

  {
    "scope": "constant.language.register.general-purpose.asm",
    "settings": {
      "foreground": "#d08f48"
    }
  },

  {
    "scope": [
      "constant.numeric.hex.asm",
      "constant.numeric.decimal.asm"
    ],
    "settings": {
      "foreground": "#d35d41"
    }
  },

  {
    "scope": "storage.type.asm",
    "settings": {
      "foreground": "#689d6a"
    }
  },

  // BATCH (bat) -------------------------------------------------------------

  {
    "scope": [
      "punctuation.definition.variable.begin.batchfile",
      "punctuation.definition.variable.end.batchfile"
    ],
    "settings": {
      "foreground": "#d08f48",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "meta.expression.set.batchfile",
      "keyword.operator.arithmetic.batchfile",
      "punctuation.definition.variable.batchfile"
    ],
    "settings": {
      "foreground": "#e0aa2b"
    }
  },

  // SHELL ----------------------------------------
  {
    "scope": "source.shell keyword.operator.logical",
    "settings": {
      "foreground": "#d35d41"
    }
  },
  {
    "scope": "punctuation.definition.logical-expression",
    "settings": {
      "foreground": "#fe8019"
    }
  },
  {
    "scope": "string.inperpolated.dollar.shell",
    "settings": {
      "foreground": "#fe8019"
    }
  },
  {
    "scope": [
      "string.interpolated.dollar.shell",
      "string.interpolated.backtick.shell"
    ],
    "settings": {
      "foreground": "#d35d41"
    }
  },

  // MAKEFILE ----------------------------------------
  {
    "scope": "meta.scope.prerequisites",
    "settings": {
      "foreground": "#fabd2f"
    }
  },
  {
    "scope": "entity.name.function.target",
    "settings": {
      "foreground": "#9e9e39",
      "fontStyle": "bold"
    }
  },
