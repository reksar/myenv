{
"name": "Motley",
"type": "dark",

"semanticHighlighting": true,
"semanticTokenColors": {
  // Python (py)
  "module:python": "#7daea3",
  "class.decorator:python": "#689d6a"
},

"tokenColors": [

  {
    "scope": [
      "emphasis"
    ],
    "settings": {
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "strong",
      "markup.bold"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "*url*",
      "*link*",
      "*uri*"
    ],
    "settings": {
      "fontStyle": "underline"
    }
  },

  {
    "settings": {
      "foreground": "#baae8f",
      "background": "#21201d"
    }
  },

  {
    "scope": [
      "punctuation.separator.dot-access",
      "punctuation.separator.pointer-access",
      "punctuation.separator.delimiter.comma",
      "punctuation.separator.namespace.access",
      "punctuation.definition.end.bracket.square",
      "punctuation.definition.begin.bracket.square",
      "punctuation.section.parens.end.bracket.round",
      "punctuation.section.parens.begin.bracket.round",
      "punctuation.section.parameters.end.bracket.round",
      "punctuation.section.parameters.begin.bracket.round",
      "punctuation.section.arguments.end.bracket.round.operator",
      "punctuation.section.arguments.begin.bracket.round.operator",
      "punctuation.section.arguments.end.bracket.round.function.call",
      "punctuation.section.arguments.begin.bracket.round.function.call",
      "punctuation.section.arguments.end.bracket.round.function.member",
      "punctuation.section.arguments.begin.bracket.round.function.member",
      // C++ (cpp)
      "punctuation.terminator.statement.cpp",
      "storage.modifier.array.bracket",
      // PHP (php)
      "keyword.operator.class.php",
      "support.constant.ext.php"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "storage.modifier",
      "storage.type.class",
      "storage.type.function",
      "storage.type.struct",
      "storage.type.enum",
      "storage.type.modifier.access.control",
      "meta.parameter.type.variable",
      "keyword.control.import",
      "keyword.control.directive.endif",
      "keyword.control.directive.pragma",
      "keyword.control.directive.define",
      "keyword.control.directive.include",
      "keyword.control.directive.conditional",
      "keyword.other.use",
      "keyword.other.namespace",
      // C++ (cpp)
      "keyword.operator.sizeof",
      // PHP (php)
      "punctuation.section.embedded.begin.php"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "keyword",
      "keyword.control",
      "keyword.operator.at",
      "keyword.operator.pipe",
      "keyword.operator.logical",
      "keyword.operator.ternary",
      "keyword.operator.assignment",
      "keyword.operator.comparison",
      "keyword.operator.conditional",
      "keyword.operator.redirection",
      "keyword.operator.bitwise.shift.cpp",
      "punctuation.definition.tag.xml",
      "punctuation.definition.directive",
      "punctuation.section.block.end.bracket.curly",
      "punctuation.section.block.begin.bracket.curly",
      "meta.parens",
      "storage.modifier.pointer",
      "storage.modifier.reference",
      "entity.name.tag.html",
      "entity.name.function.operator",
      "entity.name.function.operator.member",
      // Batch (bat)
      "meta.group.batchfile"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "keyword.other.unit.hexadecimal",
      "support.constant",
      "constant.language",
      "constant.rgb-value",
      "constant.numeric.decimal",
      "constant.character.escape",
      "string.tag",
      "string.value",
      "string.escape",
      "meta.selector",
      "entity.name.selector",
      "variable.arguments"
    ],
    "settings": {
      "foreground": "#d35d41",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "constant.numeric.hexadecimal.c",
      "constant.numeric.hexadecimal.cpp"
    ],
    "settings": {
      "foreground": "#d35d41",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "variable",
      "variable.name",
      "variable.other",
      "string.constant.other.placeholder",
      "entity.name.variable.field",
      "entity.name.variable.property",
      "entity.name.variable.local",
      "entity.name.variable.parameter",
      "entity.other.attribute-name.localname.xml",
      "meta.attribute"
    ],
    "settings": {
      "foreground": "#d08f48",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "variable.parameter"
    ],
    "settings": {
      "foreground": "#d08f48",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "entity.name.class",
      "entity.name.type.class",
      "keyword.type",
      "keyword.other.type",
      "keyword.operator.regexp",
      "support.type",
      "storage.type",
      "storage.type.string",
      "storage.type.built-in.primitive",
      "string.regexp",
      // Batch (bat)
      "variable.parameter.batchfile",
      "variable.parameter.repeat.batchfile",
      // PHP (php)
      "support.class.php",
      "support.class.exception.php"
    ],
    "settings": {
      "foreground": "#91be8d",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "string",
      "punctuation.definition.string.begin",
      "punctuation.definition.string.end",
      "entity.name.scope-resolution"
    ],
    "settings": {
      "foreground": "#ebdbb2",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "entity.name.method",
      "entity.name.method.function-call",
      "entity.name.function",
      "entity.name.function.call",
      "entity.name.function.definition",
      "support.function.magic",
      "meta.function-call.generic",
      "meta.expression.set",
      // Assembler (asm)
      "entity.name.function.asm",
      // Batch (bat)
      "keyword.other.special-method.batchfile",
      "meta.block.repeat.batchfile",
      // PHP (php)
      "support.function.constructor.php",
      "support.function.basic_functions.php",
      "support.function.url.php",
      "support.function.file.php",
      "support.function.magic.php",
      "support.function.array.php",
      "support.function.string.php",
      "support.function.filter.php",
      "support.function.errorfunc.php"
    ],
    "settings": {
      "foreground": "#afaf3f",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
    ],
    "settings": {
      "foreground": "#afaf3f",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "keyword.other.unit",
      "meta.preprocessor.string",
      "meta.preprocessor.numeric",
      "entity.name.tag.localname.xml",
      // Markdown (md)
      "markup.inserted",
      // Git
      "constant.sha.git-rebase"
    ],
    "settings": {
      "foreground": "#9e9e39"
    }
  },

  {
    "scope": [
      "support.function.builtin"
    ],
    "settings": {
      "foreground": "#9e9e39",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "punctuation.quoted",
      // Python (py)
      "source.python string.quoted.double.block.python",
      "source.python punctuation.definition.string.begin.python",
      "source.python punctuation.definition.string.end.python"
    ],
    "settings": {
      "foreground": "#777777"
    }
  },

  {
    "scope": [
      "comment",
      "punctuation.definition.comment",
      "string.quoted.docstring.multi"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "keyword.command.rem.batchfile",
      "keyword.other.phpdoc.php"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "constant.other"
    ],
    "settings": {
      "foreground": "#df6a24",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "support.other.namespace.php"
    ],
    "settings": {
      "foreground": "#e2b139",
      "fontStyle": "none"
    }
  },

  {
    "scope": [
      "entity.name.function.preprocessor",
      // Python (py)
      "punctuation.definition.decorator.python",
      // C++ (cpp)
      "meta.body.function.definition.cpp",
      // Batch (bat)
      "punctuation.definition.variable.batchfile",
      "punctuation.definition.variable.begin.batchfile",
      "punctuation.definition.variable.end.batchfile",
      "keyword.operator.arithmetic.batchfile"
    ],
    "settings": {
      "foreground": "#689d6a",
      "fontStyle": "none"
    }
  }
],
