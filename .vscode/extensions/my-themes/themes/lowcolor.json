{

"name": "Low Color",
"type": "dark",

"tokenColors": [

  {
    "settings": {
      "foreground": "#baae8f",
      "background": "#21201d"
    }
  },

  {
    "scope": "emphasis",
    "settings": {
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "string.quoted.double.json.comments"
    ],
    "settings": {
      "foreground": "#baae8f"
    }
  },

  {
    "scope": [
      "comment",
      "punctuation.definition.comment"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "italic"
    }
  },

 {
    "scope": [
      "storage.modifier.specifier",
      "storage.type.built-in.primitive",
      "storage.type.asm"
    ],
    "settings": {
      "foreground": "#827a64"
    }
  },

  {
    "scope": [
      "keyword.operator.logical",
      "keyword.operator.ternary",
      "keyword.operator.comparison",
      "storage.modifier.pointer",
      "punctuation.separator.pointer-access",
      "punctuation.separator.dot-access",
      "keyword.control.if",
      "keyword.control.return",
      "constant.character.escape.json.comments"
    ],
    "settings": {
      "foreground": "#afaf3f"
    }
  },

  {
    "scope": [
      "constant.other.placeholder",
      "constant.language.nullptr",
      "constant.language.NULL",
      "constant.numeric.decimal",
      "constant.numeric.hexadecimal",
      "constant.numeric.hex.asm",
      "keyword.other.unit.hexadecimal",
      "support.type.property-name.json.comments"
    ],
    "settings": {
      "foreground": "#d08f48"
    }
  },

  {
    "scope": [
      "keyword.operator.wordlike",
      "keyword.operator.functionlike",
      "constant.language.json.comments"
    ],
    "settings": {
      "foreground": "#b18ccf"
    }
  },

  {
    "scope": [
      "string.quoted.double"
    ],
    "settings": {
      "foreground": "#ebdbb2"
    }
  },

  {
    "scope": [
      "strong",
      "entity.name.type",
      "punctuation.section.parens.begin.bracket.round",
      "punctuation.section.parens.end.bracket.round",
      "punctuation.section.arguments.begin.bracket.round.function.call",
      "punctuation.section.arguments.end.bracket.round.function.call",
      "punctuation.section.parameters.begin.bracket.round",
      "punctuation.section.parameters.end.bracket.round",
      "punctuation.section.block.begin.bracket.curly",
      "punctuation.section.block.end.bracket.curly",
      "punctuation.definition.begin.bracket.square",
      "punctuation.definition.end.bracket.square",
      "keyword.operator.assignment",
      "punctuation.definition.dictionary.begin",
      "punctuation.definition.dictionary.end",
      "punctuation.definition.array.begin",
      "punctuation.definition.array.end",
      "punctuation.separator.dictionary.key-value",
      // Assembly (asm)
      "keyword.operator.word.mnemonic.general-purpose.data-transfer",
      "keyword.operator.word.mnemonic.general-purpose.binary-arithmetic",
      "keyword.operator.word.mnemonic.sse2.simd-integer.mov",
      "keyword.operator.word.mnemonic.general-purpose.control-transfer"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  }

],

