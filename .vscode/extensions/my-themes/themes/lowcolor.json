{

"name": "Low Color",
"type": "dark",

"tokenColors": [

  {
    "settings": {
      "background": "#121111",
      "foreground": "#BEBABA"
    }
  },

  {
    "scope": [
      "comment",
      "punctuation.definition.comment"
    ],
    "settings": {
      "foreground": "#787271",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "storage.type",
      "variable.parameter",
      "meta.parameters"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "keyword.operator"
    ],
    "settings": {
      "foreground": "#D86363",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "meta.definition.variable"
    ],
    "settings": {
      "foreground": "#5E5959",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "meta.decorator",
      "keyword.control.import",
      "keyword.control.export",
      "keyword.control.from",
      "keyword.control.loop",
      "keyword.other.haskell",
      "keyword.control.as",
      "keyword.control.conditional",
      "storage.type.function.arrow"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "punctuation",
      "meta.brace"
    ],
    "settings": {
      "foreground": "#D86363",
      "fontStyle": "#BEBABA"
    }
  },

  {
    "scope": [
      "variable",
      "meta.definition.function"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "#BEBABA"
    }
  },

  {
    "scope": [
      "meta.type",
      "meta.return.type",
      "entity.name.type"
    ],
    "settings": {
      "foreground": "#81A7BB",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "storage.modifier",
      "keyword.control.flow",
      "keyword.other.big-arrow.haskell",
      "keyword.other.arrow.haskell",
      "support.type.primitive",
      "support.type.builtin",
      "storage.type.type",
      "storage.type.interface"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "constant.character",
      "string",
      "punctuation.definition.string"
    ],
    "settings": {
      "foreground": "#8DB18A"
    }
  },

  {
    "scope": [
      "constant.character.escape"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "storage",
      "keyword.control.type",
      "keyword.operator.expression.is",
      "punctuation.definition.template-expression",
      "punctuation.terminator"
    ],
    "settings": {
      "foreground": "#BEBABA",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "constant.language.boolean",
      "constant.language.undefined",
      "constant.language.null",
      "constant.numeric"
    ],
    "settings": {
      "foreground": "#D8A763"
    }
  },

  {
    "scope": [
      "strong"
    ],
    "settings": {
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "invalid"
    ],
    "settings": {
      "foreground": "#bbbbbb88",
      "fontStyle": "italic underline"
    }
  }

],

