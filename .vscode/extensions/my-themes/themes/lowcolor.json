{
"name": "Low Color",
"type": "dark",

"semanticHighlighting": true,

"tokenColors": [
  {
    "scope": [
      "source.makefile",
      "source.php",
      "source.python",
      "source.shell",
      "source.yaml",
      "text.html.markdown"
    ],
    "settings": {
      "foreground": "#96925f"
    }
  },

  {
    "scope": [
      "constant.other",
      "meta.attribute",
      "meta.function-call",
      "meta.paragraph",
      "variable.other",
      "entity.name.function",
      "keyword.operator.arithmetic",
      "keyword.operator.assignment",
      "keyword.operator.comparison",
      "keyword.operator.key",
      "keyword.operator.logical",
      "keyword.operator.null-coalescing",
      "keyword.operator.redirect",
      "keyword.operator.string",
      "keyword.operator.ternary",
      "meta.function-call.arguments",
      "support.constant.ext",
      "support.function.array",
      "support.function.classobj",
      "support.function.constructor",
      "support.function.errorfunc",
      "support.function.file",
      "support.function.filter",
      "support.function.info",
      "support.function.streamsfuncs",
      "support.function.string",
      "support.function.url",
      "support.function.var",
      "support.function.zlib",
      "variable.parameter.function",
      // PHP (php)
      "support.function.php_pcre",
      "meta.other.type.phpdoc support.class.php",
      // YAML (yml)
      "string.unquoted.plain.out.yaml",
      // Makefile
      "meta.scope.function-call.makefile",
      "meta.scope.prerequisites.makefile",
      "meta.scope.recipe.makefile",
      "punctuation.separator.key-value.makefile",
      // Shell (sh)
      "meta.scope.if-block.shell",
      "meta.scope.while-loop.shell",
      // Python (py)
      "keyword.operator.python"
    ],
    "settings": {
      "foreground": "#baae8f"
    }
  },

  {
    "scope": [
      "keyword.control.foreach",
      "keyword.control.import",
      "keyword.other.namespace",
      "keyword.other.use",
      "keyword.other.use-as",
      "storage.modifier",
      "storage.type.class",
      "storage.type.const",
      "storage.type.function",
      "storage.type.trait",
      "support.function.basic_functions",
      "support.function.construct",
      // PHP (php)
      "punctuation.section.embedded.begin.php",
      "storage.type.php",
      "variable.language.this",
      // Makefile
      "support.function.shell.makefile",
      "support.function.target.PHONY.makefile",
      // Shell (sh)
      "support.function.builtin.shell"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "italic"
    }
  },

  {
    "scope": [
      "keyword.control.else",
      "keyword.control.exception",
      "keyword.control.flow",
      "keyword.control.if",
      "keyword.control.return",
      "keyword.other.new",
      // PHP (php)
      "keyword.control.die.php",
      // Shell (sh)
      "keyword.control.shell",
      // Dockerfile
      "keyword.other.special-method.dockerfile",
      // YAML (yml)
      "entity.name.tag.yaml",
      // Makefile
      "entity.name.function.target.makefile",
      // Markdown (md)
      "entity.name.section.markdown"
    ],
    "settings": {
      "foreground": "#baae8f",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      "string.quoted.double",
      "string.quoted.single",
      "string.regexp.double-quoted",
      "string.regexp.single-quoted",
      "punctuation.definition.string",
      // Constants
      "constant.numeric.dec",
      "constant.numeric.decimal",
      "constant.language",
      "string.regexp.character-class",
      // Makefile
      "meta.scope.prerequisites.makefile",
      // Markdown (md)
      "markup.inline.raw.string",
      "markup.fenced_code.block"
    ],
    "settings": {
      "foreground": "#edcd98"
    }
  },

  {
    "scope": [
      "keyword.operator.class",
      "keyword.operator.error-control",
      "keyword.operator.unpacking",
      "punctuation.accessor",
      "punctuation.parenthesis",
      "punctuation.definition.arguments",
      "punctuation.definition.begin.bracket",
      "punctuation.definition.block",
      "punctuation.definition.class.begin.bracket",
      "punctuation.definition.class.end.bracket",
      "punctuation.definition.dict",
      "punctuation.definition.list",
      "punctuation.definition.end.bracket",
      "punctuation.definition.inheritance",
      "punctuation.definition.heading",
      "punctuation.definition.mapping",
      "punctuation.definition.raw",
      "punctuation.definition.variable",
      "punctuation.definition.parameters",
      "punctuation.section.array",
      "punctuation.section.class",
      "punctuation.section.function",
      "punctuation.separator.arguments",
      "punctuation.separator.annotation.result",
      "punctuation.separator.colon",
      "punctuation.separator.comma",
      "punctuation.separator.delimiter",
      "punctuation.separator.dict",
      "punctuation.separator.element",
      "punctuation.separator.parameters",
      "punctuation.separator.period",
      "punctuation.terminator.statement",
      "constant.character.format.placeholder.other",
      // Markdown (md)
      "punctuation.definition.link.title.begin.markdown",
      "punctuation.definition.link.title.end.markdown",
      "punctuation.definition.metadata.markdown",
      // PHP (php)
      "keyword.operator.return-value.php",
      "punctuation.separator.inheritance.php",
      "punctuation.terminator.expression.php",
      // YAML (yml)
      "punctuation.separator.key-value.mapping.yaml",
      "punctuation.definition.block.sequence.item.yaml",
      // Shell (sh)
      "punctuation.definition.logical-expression.shell",
      // Javascript (js)
      "meta.brace.round.js",
      "meta.brace.square.js"
    ],
    "settings": {
      "foreground": "#aa8060"
    }
  },

  {
    "scope": [
      "comment",
      "punctuation.definition.comment",
      "string.quoted.docstring.multi"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "italic"
    }
  },


  {
    "scope": [
      "support.class",
      "entity.name.type.class",
      "entity.name.type.trait",
      "entity.other.inherited-class",
      "keyword.other.type",
      // YAML (yml)
      "constant.numeric.integer.yaml",
      "entity.other.document.begin.yaml",
      // Shell (sh)
      "string.other.math.shell",
      "string.interpolated.dollar.shell"
    ],
    "settings": {
      "foreground": "#ccc281"
    }
  },

  {
    "scope": [
      "constant.character.escape",
      "keyword.operator.regexp",
      "string.regexp.character-class",
      "storage.type.string"
    ],
    "settings": {
      "foreground": "#ccc281",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      // PHP (php)
      "keyword.other.phpdoc"
    ],
    "settings": {
      "foreground": "#777777",
      "fontStyle": "bold"
    }
  },

  {
    "scope": [
      // Markdown (md)
      "string.other.link.title.markdown"
    ],
    "settings": {
      "foreground": "#449aba"
    }
  },

  {
    "scope": [
      "markup.underline.link"
    ],
    "settings": {
      "foreground": "#449aba",
      "fontStyle": "underline"
    }
  }
],

